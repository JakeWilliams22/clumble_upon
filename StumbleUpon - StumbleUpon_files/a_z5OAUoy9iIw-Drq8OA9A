        return function() {
          return _this.save();
        };
      })(this));
      return this.on('change:_seen', (function(_this) {
        return function() {
          if (!_this.get('_byid')) {
            return _this.save();
          }
        };
      })(this));
    }
  };

  StumbleModel.prototype.getInterestName = function() {
