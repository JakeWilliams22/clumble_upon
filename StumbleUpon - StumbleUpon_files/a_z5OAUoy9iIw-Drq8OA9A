        return this.model.set('_class', 'size-4');
      case 30:
        return this.model.set('_class', 'size-5');
      case 38:
        return this.model.set('_class', 'size-6');
      default:
        return this.model.set('_class', 'size-1');
    }
  };

  return TagCloudItemView;

