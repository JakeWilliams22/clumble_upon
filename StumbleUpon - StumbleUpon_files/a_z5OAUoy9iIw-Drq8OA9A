    }
    this.resetList();
    return this.setTimeout((function(_this) {
      return function() {
        var _ref1;
        return (_ref1 = _this.$el) != null ? _ref1.find('.dropdown-search').focus() : void 0;
      };
    })(this), 250);
  };

  AddToListView.prototype.afterCollectionComplete = function() {
