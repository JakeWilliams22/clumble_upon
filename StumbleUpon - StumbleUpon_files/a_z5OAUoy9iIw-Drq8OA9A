    var _ref, _ref1;
    if ((_ref = this.$el) != null) {
      _ref.find('.send-spin').removeClass('hidden');
    }
    return (_ref1 = this.$el) != null ? _ref1.find('.send-report').addClass('disabled') : void 0;
  };

