    return function() {
      var _ref;
      if (decoratedMethod) {
        return decoratedMethod.apply(this, arguments);
      } else {
        return (_ref = this.$el) != null ? _ref.parent() : void 0;
      }
    };
  },
  onScroll: function(decoratedMethod) {
    return function(e) {
