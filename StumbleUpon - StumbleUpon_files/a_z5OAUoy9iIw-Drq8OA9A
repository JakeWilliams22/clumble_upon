    ContactView.__super__.render.apply(this, arguments);
    return this.subview('thumbnail', new ParticipantThumbnailView({
      container: this.$el.find('.contact-thumbnail'),
      model: this.model,
      hideDefaultThumbnail: false
    }));
  };

  return ContactView;

})(View);


