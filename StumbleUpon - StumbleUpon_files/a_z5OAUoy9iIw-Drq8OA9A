  getDiff: function(base, updated) {
    var diff;
    diff = {};
    $.each(updated, function(key, value) {
      if (base.hasOwnProperty(key)) {
        if (updated[key] !== base[key]) {
          return diff[key] = updated[key];
        }
      }
    });
