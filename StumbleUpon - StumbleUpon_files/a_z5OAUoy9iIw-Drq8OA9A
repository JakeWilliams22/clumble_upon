  loadAll: function() {
    var s, wf;
    wf = document.createElement("script");
    wf.src = "//ajax.googleapis.com/ajax/libs/webfont/1/webfont.js";
    wf.type = "text/javascript";
    wf.async = "true";
    s = document.getElementsByTagName("script")[0];
    s.parentNode.insertBefore(wf, s);
    return null;
  }
};


